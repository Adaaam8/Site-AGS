{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/favicon.ico",
      "destination": "/assets/favicon.png"
    },
    {
      "source": "/:path((?!sitemap\\.xml|robots\\.txt|favicon).*)",
      "destination": "/index.html"
    }
  ]
}